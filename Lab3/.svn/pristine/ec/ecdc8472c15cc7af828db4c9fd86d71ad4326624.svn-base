<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE html>

<html xmlns="http://www.w3.org/1999/xhtml"
      xmlns:h="http://xmlns.jcp.org/jsf/html"
      xmlns:ui="http://xmlns.jcp.org/jsf/facelets">

<ui:composition template="WEB-INF/templates/creator.xhtml">

    <ui:define name="head">
        <h:outputStylesheet library="css" name="index.css"/>
        <title> Web Project 3 - Start Page</title>
    </ui:define>

    <ui:define name="main">
        <main class="main-container">
            <h1 class="visually-hidden">Start Page</h1>
            <section class="main-container-item main-container-item-start">
                <h2 class="visually-hidden">Start</h2>

                <p class="date-time">
                    <span class="date-time-date"></span><br/>
                    <span class="date-time-time"></span>
                </p>
                <div class="index-list">
                    <h:link styleClass="index-list-link index-list-link-to-main" outcome="toMain">
                        To the Main Page
                    </h:link>
                </div>
            </section>
        </main>

        <h:outputScript library="js" name="index.js"/>
    </ui:define>
</ui:composition>
</html>

